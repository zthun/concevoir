{
  "name": "@zthun/fashion",
  "version": "0.0.0",
  "description": "Frontend library used to design and layout web pages.",
  "author": "Anthony Bonta",
  "license": "MIT",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/fashion"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf --glob 'coverage' '*.tgz' '*.log' 'packages/**/dist' 'packages/**/*.tgz' 'packages/**/*.log' 'packages/**/stats'",
    "check": "tsc",
    "test": "vitest run --coverage",
    "e2e": "cucumber-js",
    "lint": "lint-janitor",
    "build": "lerna run build",
    "make": "yarn clean && yarn check && yarn lint && yarn test && yarn build",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@cucumber/cucumber": "^11.1.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/stylis": "^4.2.7",
    "@vitest/coverage-istanbul": "^2.1.8",
    "@zthun/lint-janitor": "^18.0.5",
    "@zthun/lint-janitor-config": "^18.0.5",
    "happy-dom": "^16.0.1",
    "husky": "^9.1.7",
    "lerna": "^8.1.9",
    "lodash-es": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.13.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "packageManager": "yarn@4.6.0"
}
