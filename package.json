{
  "name": "@zthun/fashion",
  "version": "0.0.0",
  "description": "Frontend library used to design and layout web pages.",
  "author": "Anthony Bonta",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/fashion"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf --glob 'coverage' '*.tgz' '*.log' 'packages/**/dist' 'packages/**/*.tgz' 'packages/**/*.log' 'packages/**/stats'",
    "test": "vitest run --coverage",
    "e2e": "cucumber-js",
    "check": "tsc --noEmit",
    "lint": "lint-janitor",
    "build": "lerna run build",
    "make": "yarn clean && yarn lint && yarn check && yarn test && yarn build",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@cucumber/cucumber": "^10.5.1",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.17.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@types/react-window": "^1.8.8",
    "@vitest/coverage-istanbul": "^1.5.0",
    "@zthun/lint-janitor": "^16.1.0",
    "@zthun/lint-janitor-config": "^16.1.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^8.1.2",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-dts": "^3.8.3",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.0"
  },
  "packageManager": "yarn@4.1.1"
}
