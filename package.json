{
  "name": "@zthun/fashion",
  "version": "0.0.0",
  "description": "Frontend library used to design and layout web pages.",
  "author": "Anthony Bonta",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/fashion"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf --glob 'coverage' '*.tgz' '*.log' 'packages/**/dist' 'packages/**/*.tgz' 'packages/**/*.log'",
    "test": "vitest run --coverage",
    "e2e": "cucumber-js",
    "lint": "lint-janitor",
    "build": "lerna run build",
    "make": "yarn clean && yarn lint && yarn test && yarn build",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@cucumber/cucumber": "^9.4.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.0",
    "@types/react-window": "^1.8.5",
    "@types/uuid": "^9.0.2",
    "@vitest/coverage-istanbul": "^0.34.1",
    "@zthun/lint-janitor": "^14.3.1",
    "@zthun/lint-janitor-config": "^14.3.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^7.1.5",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  },
  "packageManager": "yarn@3.3.1"
}
