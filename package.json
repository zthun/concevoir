{
  "name": "@zthun/fashion",
  "version": "0.0.0",
  "description": "Frontend library used to design and layout web pages.",
  "author": "Anthony Bonta",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/fashion"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf --glob 'coverage' '*.tgz' '*.log' 'packages/**/dist' 'packages/**/*.tgz' 'packages/**/*.log'",
    "test": "vitest run --coverage",
    "e2e": "cucumber-js",
    "lint": "lint-janitor",
    "build": "lerna run build",
    "make": "yarn clean && yarn lint && yarn test && yarn build",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@cucumber/cucumber": "^9.0.0",
    "@types/jsdom": "^21.1.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@types/uuid": "^9.0.1",
    "@vitest/coverage-istanbul": "^0.29.2",
    "@zthun/lint-janitor": "^14.1.0",
    "@zthun/lint-janitor-config": "^14.1.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lerna": "^6.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.4.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.29.2"
  },
  "packageManager": "yarn@3.3.1"
}
