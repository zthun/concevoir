{
  "name": "@zthun/fashion",
  "version": "0.0.0",
  "description": "Frontend library used to design and layout web pages.",
  "author": "Anthony Bonta",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/fashion"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf --glob 'coverage' '*.tgz' '*.log' 'packages/**/dist' 'packages/**/*.tgz' 'packages/**/*.log'",
    "test": "vitest run --coverage",
    "e2e": "cucumber-js",
    "lint": "lint-janitor",
    "build": "lerna run build",
    "make": "yarn clean && yarn lint && yarn test && yarn build",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@cucumber/cucumber": "^10.0.1",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.4",
    "@types/react-window": "^1.8.8",
    "@vitest/coverage-istanbul": "^1.0.4",
    "@zthun/lint-janitor": "^15.1.2",
    "@zthun/lint-janitor-config": "^15.1.2",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "jsdom-global": "^3.0.2",
    "lerna": "^8.0.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.3.3",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.0.4"
  },
  "packageManager": "yarn@3.3.1"
}
